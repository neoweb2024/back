{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/routes/index.js"
        },
        {
            "src": "/appointment",
            "dest": "/routes/appointment.js"
        },
        {
            "src": "/images",
            "dest": "/routes/images.js"
        },
        {
            "src": "/availability",
            "dest": "/routes/availability.js"
        },
        {
            "src": "/mercadopago",
            "dest": "/routes/mp.js"
        },
        {
            "src": "/users",
            "dest": "/routes/users.js"
        }
    ]
}